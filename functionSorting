let arrayToSort = []
for(let i = 0 ; i < 10; i++) {
    arrayToSort[i] = Math.round(Math.random() * 100 - 50)
}

function sorting(arrayToSort){
    for(let j = 0; j < arrayToSort.length - 1; j++) {
        for(let i = j + 1; i < arrayToSort.length; i++) {
            if(arrayToSort[j] > arrayToSort[i]) {
                let temp = arrayToSort[j]
                arrayToSort[j] = arrayToSort[i]
                arrayToSort[i] = temp
            }
        }
    }
return arrayToSort
}


function sorted() {
    let sortedNumArr = sorting(arrayToSort)
    console.log("Sorteed:", sortedNumArr)
}

sorted();
